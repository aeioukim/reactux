{
  "name": "reactux-backend",
  "version": "3.1.0",
  "description": "React + Redux + Hot + Webpack + Express Template",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-register",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js",
    "dev": "NODE_ENV=dev nodemon bin/www-dev --exec babel-node",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "clean": "rm -rf dist",
    "deploy": "npm run build && pm2 start server.json"
  },
  "author": "haochuan",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.14.1",
    "morgan": "^1.7.0",
    "node-basicauth": "0.0.9",
    "nodemon": "^1.11.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  }
}
