{
  "name": "reactux",
  "version": "4.0.0",
  "description": "React + Redux + HOC + Webpack + Express Template",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "npm test -- --watch",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js",
    "dev": "NODE_ENV=dev ./node_modules/.bin/babel-node ./src/server/index.js",
    "prod": "npm run build && NODE_ENV=production node ./build/server/index.js",
    "build": "./node_modules/.bin/rimraf ./build && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors && npm run buildServer",
    "buildServer": "babel src/server -s -D -d build/server",
    "deploy": "npm run build && pm2 start server.json"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "haochuan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "helmet": "^3.4.0",
    "lodash": "^4.14.1",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "antd": "^2.8.0",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^16.0.0",
    "jsdom": "^9.4.2",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "stats-webpack-plugin": "^0.4.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
