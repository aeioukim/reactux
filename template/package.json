{
  "name": "reactux",
  "version": "4.0.3",
  "description": "React + Redux + HOC + Webpack + Express Template",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "npm test -- --watch",
    "lint": "./node_modules/.bin/eslint ./src/**/*.js",
    "dev": "NODE_ENV=dev ./node_modules/.bin/babel-node ./src/server/index.js",
    "prod": "npm run build && NODE_ENV=production node ./build/server/index.js",
    "build": "./node_modules/.bin/rimraf ./build && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors && npm run buildServer",
    "buildServer": "babel src/server -s -D -d build/server",
    "deploy": "npm run build && pm2 start server.json"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "haochuan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "antd": "^2.12.1",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "jsdom": "^11.1.0",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
